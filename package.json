{
	"name": "96jd-global-eye",
	"displayName": "96JD Global Eye",
	"description": "Search News API For Articles",
	"version": "1.0.3",
	"publisher": "JacobDolorzo",
	"repository": {
		"url": "https://github.com/96JD/96jd-global-eye"
	},
	"icon": "logo.png",
	"engines": {
		"vscode": "^1.92.1"
	},
	"categories": [
		"Other"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "96jd-global-eye.searchNA",
				"title": "96JD: Global Eye"
			}
		]
	},
	"scripts": {
		"postinstall": "husky",
		"build-base": "esbuild extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
		"build": "yarn build-base --sourcemap",
		"build-watch": "yarn build-base --sourcemap --watch",
		"vsce:prepublish": "yarn build-base --minify",
		"vsce:publish": "vsce publish",
		"lint": "eslint ."
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"axios": "^1.7.3",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@types/lodash": "^4.17.7",
		"@types/node": "^22.1.0",
		"@types/vscode": "^1.92.0",
		"@typescript-eslint/eslint-plugin": "^8.0.1",
		"@typescript-eslint/parser": "^8.0.1",
		"@vscode/vsce": "^2.31.1",
		"esbuild": "^0.23.0",
		"eslint": "^8.57.0",
		"husky": "^9.1.4",
		"lint-staged": "^15.2.8",
		"prettier": "^3.3.3",
		"typescript": "^5.5.4",
		"yarn-upgrade-all": "^0.7.4"
	}
}
